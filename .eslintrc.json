{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "import/prefer-default-export": "off",
        "eqeqeq": "off",
        "no-await-in-loop": "off",
        "prettier/prettier": "error"
    }
}
